<?xml version="1.0" encoding="ISO-8859-1"?>

<h1>Tabela de resultados</h1>

<table class="resultTable">
    <tr>
        <th>A</th>
        <th>B</th>
        <th>xn</th>
        <th>f(x)</th>
    </tr>
    <% for(var i = 0; i < data.length; i++) { %>
        <tr>
            <td>data[i].a</td>
            <td>data[i].b</td>
            <td>data[i].xn</td>
            <td>data[i].fx</td>
        </tr>     
    <% } %>
</table>